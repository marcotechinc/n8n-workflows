    {
      "parameters": {
        "jsCode": "// Code node\n// Groups items by cluster_id and keeps topic\n\nconst clusters = {};\n\nitems.forEach(item => {\n  const cid = item.json.cluster_id ?? 0;\n  const m = item.json.metadata || {};\n\n  const entry = {\n    id: item.json.id,\n    source: m.source || \"unknown\",\n    date: m.date || \"unknown\",\n    topic: m.topic || \"unknown\",        // ← FIX\n    category: m.category || \"unknown\",\n    content: item.json.content\n  };\n\n  (clusters[cid] ||= []).push(entry);\n});\n\nreturn Object.entries(clusters).map(([cluster_id, entries]) => ({\n  json: {\n    cluster_id: Number(cluster_id),\n    correlated_count: entries.length,\n    items: entries\n  }\n}));"
      },
