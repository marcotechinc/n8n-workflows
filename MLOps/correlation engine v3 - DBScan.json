    {
      "parameters": {
        "jsCode": "const raw = $json.text;\n\nif (!raw) {\n  return [];\n}\n\nlet incidents;\ntry {\n  incidents = JSON.parse(raw);\n} catch (e) {\n  throw new Error('Invalid JSON from LLM');\n}\n\nif (!Array.isArray(incidents) || incidents.length === 0) {\n  return [];\n}\n\nreturn incidents.map(i => ({\n  json: {\n    incident_key: i.incident_key,\n    topic: i.topic,\n    title: i.incident_title,\n    summary: i.incident_summary,\n    signals: i.signals,\n    confidence: i.confidence\n  }\n}));"
      },
